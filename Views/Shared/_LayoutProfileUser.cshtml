
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile </title>
    <link href="~/CSS/ProfileUser.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.0/dist/css/bootstrap.min.css" rel="stylesheet">
</head>

<body>
    <div class="container light-style flex-grow-1 container-p-y">
        <h4 class="font-weight-bold py-3 mb-4">
            Account settings
        </h4>
        <div class="card overflow-hidden">
            <div class="row no-gutters row-bordered row-border-light">
                <div class="col-md-3 pt-0">
                    <div class="list-group list-group-flush account-settings-links">
                        <a class="list-group-item list-group-item-action "
                           href="@Url.Action("Index", "ProfileUser")">General</a>
                        <a class="list-group-item list-group-item-action "
                           href="@Url.Action("ChangePassword", "ProfileUser")">Change Password</a>
                        <a class="list-group-item list-group-item-action"
                           href="@Url.Action("ShowTickets", "ProfileUser")">Ticket</a>
                        <a class="list-group-item list-group-item-action"
                           href="@Url.Action("Index", "Home")">Home</a>
                        <a class="list-group-item list-group-item-action"
                           href="@Url.Action("Logout", "Home")">Log Out</a>

                        
                    </div>
                </div>
                <div class="col-md-9">
                    <div class="tab-content">

                        @RenderBody()



                    </div>
                </div>
            </div>
        </div>
    </div>
    <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
    <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
    <script type="text/javascript">

    </script>
</body>

</html>